
@*@using StateManagementDemo.Models;*@

<h1>@ViewBag.username</h1>
<hr />

<ul>
    @foreach (var item in ViewBag.usersData)
    {
        <li>@item.UserName  |  @item.Password</li>

    }
</ul>
<hr />

<h1>@ViewData["newname"]</h1>
<ul>
    @foreach (UserViewModel item in (List<UserViewModel>)ViewData["newUserList"])
    {
        <li>@item.UserName  |  @item.Password</li>
    }
</ul>